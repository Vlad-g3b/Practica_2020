         <children>
            <JFXPasswordField fx:id="old_password" alignment="CENTER" labelFloat="true" promptText="Old Password" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="1">
               <styleClass>
                  <String fx:value="text_font" />
                  <String fx:value="other_fields" />
