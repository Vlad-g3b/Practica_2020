<AnchorPane prefHeight="300.0" prefWidth="398.0" stylesheets="@assets/style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ChangePass">
