            <JFXPasswordField fx:id="new_password" alignment="CENTER" labelFloat="true" prefHeight="71.0" prefWidth="265.0" promptText="New Password" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="3">
